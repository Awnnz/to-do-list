{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-use-before-define": "warn",
    "no-restricted-syntax": "warn",
    "guard-for-in": "warn",
    "no-plusplus": "off"
  }
}
